
<div class="page">
    <div class="page content">
        @ChildContent
    </div>
    @if (PageNumber > 0)
    {
        <div class="page number">
            - @PageNumber -
        </div>
    }
</div>

@code {
    [Parameter] public RenderFragment? ChildContent { get; set; }
    [Parameter] public int PageNumber { get; set; }

}
